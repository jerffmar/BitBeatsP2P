{
  "name": "bitbeats-monolith",
  "version": "1.0.0",
  "description": "BitBeats: A decentralized, hybrid P2P music streaming PWA.",
  "main": "src/server.ts",
  "scripts": {
    "start": "ts-node src/server.ts",
    "dev": "ts-node-dev --respawn src/server.ts",
    "postinstall": "npm run client:install && npx prisma generate",
    "client:install": "node scripts/client-install.cjs",
    "client:build": "node scripts/client-build.cjs",
    "build": "npm run client:build",
    "db:migrate": "npx prisma migrate dev --name init",
    "db:seed": "ts-node prisma/seed.ts"
  },
  "keywords": [
    "p2p",
    "webtorrent",
    "react",
    "express",
    "sqlite"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@mapbox/node-pre-gyp": "^1.0.11",
    "@prisma/client": "^5.10.2",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "webtorrent-hybrid": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.24",
    "prisma": "^5.10.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
